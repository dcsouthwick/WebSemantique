@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos/> .
@prefix ns1: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix tp1: <http://cui.unige.ch/tws/enriched_trails/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

tp1:Track a rdfs:Class ;
    rdfs:range tp1:Place ;
    rdfs:subClassOf rdfs:Resource .

geo:alt rdfs:domain geo:SpatialThing .

geo:lat rdfs:domain geo:SpatialThing .

geo:location rdfs:range geo:SpatialThing .

geo:long rdfs:domain geo:SpatialThing .

geo:Point rdfs:domain tp1:Place ;
    rdfs:subClassOf geo:SpatialThing .

tp1:Place a ns1:Class ;
    rdfs:label tp1:label ;
    rdf:Property geo:Point,
        schema:address ;
    rdf:langString tp1:label ;
    rdfs:domain tp1:Track ;
    ns1:equivalentClass schema:Place .

geo:SpatialThing rdfs:subClassOf rdfs:Resource .

